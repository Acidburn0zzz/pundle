{
  "dependencies": {
    "babel-preset-steelbrain": "^4.0.1",
    "eslint-config-steelbrain": "^1.0.4",
    "lerna": "2.0.0-beta.24",
    "ucompiler": "^3.1.9",
    "ucompiler-plugin-babel": "^3.1.1"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap; npm run compile",
    "clean": "rm -rf packages/*/{lib,node_modules}",
    "compile": "npm run clean; lerna exec -- ucompiler go",
    "lint": "eslint .",
    "publish": "npm run compile; lerna publish",
    "test": "lerna exec --concurrency 1 -- apm test",
    "watch": "npm run clean; lerna exec -- ucompiler watch"
  }
}
